// 函数定义测试
int add(int a, int b) {
    return a + b;
}

float multiply(float x, float y) {
    return x * y;
}

// 数组测试
int fibonacci(int n) {
    int arr[5] = {1, 1, 2, 3, 5};  // 正常数组
    if (n < 5) {
        return arr[n];
    }
    return 0;
}

// 控制流测试
int factorial(int n) {
    if (n <= 1) {
        return 1;
    }
    
    int result = 1;
    while (n > 1) {
        result = result * n;
        n = n - 1;
    }
    
    return result;
}

// 主函数
